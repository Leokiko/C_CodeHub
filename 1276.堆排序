#include<stdio.h>
int main() {
	int a[1000]= {0};
	int n;
	scanf("%d",&n);
	for(int i=0; i<n; i++) {
		scanf("%d",&a[i]);
	}
	for(int i=0; i<n; i++) {
		int t;
		for(int j=i+1; j<n; j++) {
			if(a[j]<a[i]) {
				t=a[i];
				a[i]=a[j];
				a[j]=t;
			}
		}
	}
	for(int i=0; i<n-1; i++) {
		printf("%d ",a[i]);
	}
	printf("%d",a[n-1]);
	return 0;
}